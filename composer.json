{
    "name": "tigron/skeleton-i18n",
    "description": "Translation and internationalization for Skeleton",
    "type": "library",
    "require": {
		"tigron/skeleton-migrate": "*",
		"tigron/skeleton-object": "*",
		"tigron/skeleton-database": "*",
		"tigron/skeleton-core": ">=3"
    },
    "suggest": {
        "guzzlehttp/guzzle": "The translator services require the Guzzle api client",
        "ext-xml": "Needed to use the translation services"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tigron BVBA",
            "email": "support@tigron.be"
        }
    ],
    "support": {
        "source": "https://github.com/tigron/skeleton-i18n",
        "issues": "https://github.com/tigron/skeleton-i18n/issues"
    },
    "autoload": {
        "psr-4": { "Skeleton\\I18n\\": "lib/Skeleton/I18n/" }
    }
}
